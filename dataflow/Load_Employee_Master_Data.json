{
	"name": "Load_Employee_Master_Data",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Dataset_ADLSGen2_EmpMaster",
						"type": "DatasetReference"
					},
					"name": "SourceEmpMaster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_nyc_payroll_emp_md",
						"type": "DatasetReference"
					},
					"name": "SinkEmpMaster"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          EmployeeID as string,",
				"          LastName as string,",
				"          FirstName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SourceEmpMaster",
				"SourceEmpMaster sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          EmployeeID as string,",
				"          LastName as string,",
				"          FirstName as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          EmployeeID,",
				"          LastName,",
				"          FirstName",
				"     )) ~> SinkEmpMaster"
			]
		}
	}
}